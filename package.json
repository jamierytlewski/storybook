      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/genesis-storybook.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/genesis-storybook.umd.cjs"
      }
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/genesis-storybook.umd.cjs",
  "module": "./dist/genesis-storybook.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "description": "RPM GENESIS-UI Component Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loadrpm/genesis-storybook.git"
